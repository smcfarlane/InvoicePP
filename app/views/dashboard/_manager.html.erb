<div class="container">
  <div class="row panel panel-default">
    <div class="panel-heading"><h3>Manager Items</h3></div>
    <div class="panel-body">
      <% u.clients.all.each do |c| %>
        <div class="col-md-4">
          <h4><%=	 c.detail.name %></h4>
          <ul>
            <%	 c.projects.all.each do |p| %>
              <li><%=	p.name %> - <%= p.identifier %></li>
            <%	 end %>
          </ul>
        </div>
      <% end %>
    </div>
    <div class="panel-footer">
      <div class="btn-group">
        <%= link_to 'All Clients', clients_path, class: "btn btn-default" %>
        <%= link_to 'Create New Client', new_client_path, class: "btn btn-default" %>
      </div>
    </div>
  </div>
</div>